#include <stdio.h>
#include <string.h>
#include <math.h> 
int main()
{
  	double x,y;
  	double area,s;
  	printf("请输入月用电量：\n");
  	scanf("%lf",&x);
  	

	if(x<50)
	  {
	  	y=0.53*x;
	  		printf("应支付电费=%.2f\n",y);	
	  }
	  else 
	  {
	  	y=50*0.53+(x-50)*0.58;
	  		printf("应支付电费=%.2f\n",y);	
	  }
	  
    return 0;
}
