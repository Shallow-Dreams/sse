#include  <stdio.h>
#define N 40
#include <string.h>
	int a[13];
int main()
{
	
	int m,i,t;
	int b[13];
    a[0]=1;
    t=1;
    m=0;
    	for(i=1;i<=13;)
    	{
    		if(m>12)
    		{
    			m=m-13;
			}
			
			a[m]=i;
			i++;
			m=m+i;
			t++;
			if(t==14)
			{
				break;
			}
		}
	
		printf("The original order of cards is:");
		printf("1 8 2 5 10 3 12 11 9 4 7 6 13");
		
		
		printf("\n");

    return 0;
}


