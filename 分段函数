#include <stdio.h>
#include <string.h>
#include <math.h> 
int main()
{
  	float x,y;
  	double area,s;
  	printf("Please input x:");
  	scanf("%f",&x);
  	
  	if( x<1)
	  {	  
	  	 y= x;  
		printf("y = %.2f\n",y);			  		  		  
	  }
	  else if(x>=1 && x<10)
	  {
	  	y= 2*x-1 ;
	  		printf("y = %.2f\n",y);	
	  }
	  else if( x>=10)
	  {
	  	 y= 3*x-11           ;
	  		printf("y = %.2f\n",y);	
	  }
	  
    return 0;
}
